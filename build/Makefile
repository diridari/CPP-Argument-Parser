all::
	make -f Makefile.test
	make -f Makefile.app1
	make -f Makefile.app2

test :
	make -f Makefile.test
	./CPP-Argument-Parser-Test

clean ::
	make  -f  Makefile.test clean
	make  -f  Makefile.app clean



